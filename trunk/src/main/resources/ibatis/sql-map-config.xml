<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

	<settings
		cacheModelsEnabled="true"
		enhancementEnabled="true"
		lazyLoadingEnabled="true"
		maxRequests="512"
		maxSessions="128"
		maxTransactions="32"
		useStatementNamespaces="true"
	/>
	
    <typeAlias alias="str" type="com.google.code.booktogether.dao.ibatis.StrDomain"/>
	<typeAlias alias="num" type="com.google.code.booktogether.dao.ibatis.NumDomain"/>

 	<sqlMap resource="bestsellerssqls.xml"/>
 	<sqlMap resource="blogsqls.xml"/>
 	<sqlMap resource="bookmarksqls.xml"/>
 	<sqlMap resource="booksqls.xml"/>
 	<sqlMap resource="goodwritersqls.xml"/>
 	<sqlMap resource="gradesqls.xml"/>
 	<sqlMap resource="librarybooksqls.xml"/>
 	<sqlMap resource="libraryboardsqls.xml"/>
 	<sqlMap resource="possessbooksqls.xml"/>
 	<sqlMap resource="recobooksqls.xml"/>
 	<sqlMap resource="reviewsqls.xml"/>
 	<sqlMap resource="usersqls.xml"/>
 	
</sqlMapConfig>
